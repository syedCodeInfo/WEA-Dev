@model IEnumerable<WEA.FAQServices.Collabration.Abstraction.OutdoorRelay.FAQDetails>

@{
    ViewData["Title"] = "ViewAllRespondedFAQ";
    Layout = "~/Views/Shared/_userLayout.cshtml";
    int id = 0;
}
  <section class="section">
      <div class="row">
       <div class="col-lg-12">
               <div class="card basic">
            <div class="card-body">
              <h5 class="card-title">View All FAQ</h5>
               @foreach (var item in Model) {
                     id++;
              <div>
                <h6>@id . @Html.DisplayFor(modelItem => item.Question) - @Html.DisplayFor(modelItem => item.CreatedAt)</h6>
                <p> @Html.DisplayFor(modelItem => item.Answer)</p>
              </div>

         
              }
            </div>
          </div>
        
        </div>
      </div>
    </section>


